<h1>General Search</h1>
<div id="SearchContainer">
	<div id="SearchBar">
		<%= form_tag({:controller => "search", :action => "show"}, :method => "get", :class => "generic-search") do %>
		  <%= label_tag(:q, "Search for:") %>
		  <%= text_field_tag(:q) %>
		  <%= submit_tag "Search", :name => nil %>
		<% end %>
	</div>	
</div>
